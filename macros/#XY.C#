#include <iostream>
#include <fstream>
#include <iomanip>
#include <vector>
#include <string>

void XY(){
  int i, j;
  const int n = 8;	
  float x[n], y[n];

  ifstream data;

  string name = "gluinoXsection";
  data.open((name+".txt").c_str());

  int k = 0;

  TH2F * hist = new TH2F("","",20,8.0,10.0,100,0,100);

  while(!data.eof() && k<n){
    data>>x[k]>>y[k];
    cout<<x[k]<<" "<<y[k]<<endl;
    hist->Fill(x[k],y[k]);
    k++;
  }
  
  hist->SetMinimum(0); 
  hist->SetMaximum(100);
  hist->Draw();
  hist->SetDrawOption("COLZ");
  hist->GetXaxis()->SetLabelSize(0.035);

  
}
