#!/bin/tcsh -x

setenv JobName -filename-.JobName

set casa = `pwd`
echo $casa
setenv CORAL_AUTH_USER ""
setenv CORAL_AUTH_PASSWORD ""

cd -padtorun-
echo `pwd`
project CMSSW
eval `scramv1 ru -csh`

cd -currentpad-
mv -filename-.cfg $casa
cd $casa

cmsRun -filename-.cfg > & output.txt

rfcp MuonSegEff.-filename- -storepad-

exit 0

